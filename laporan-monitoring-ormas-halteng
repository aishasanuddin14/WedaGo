    function isActive(status) {
      if (!status) return false;
      const s = String(status).toLowerCase();
      return s.includes("aktif") && !s.includes("tidak");
    }

    function summarize(rows) {
      const total = rows.length;
      const aktif = rows.filter(r => isActive(r["Status"])).length;
      const non = rows.filter(r => (r["Status"]||"").toLowerCase().includes("tidak")).length;
      const tereg = rows.filter(r => (r["Status Pendaftaran"]||"").toLowerCase().includes("skt") || (r["Status Pendaftaran"]||"").toLowerCase().includes("kemenkumham")).length;
      document.getElementById("statTotal").innerText = total;
      document.getElementById("statAktif").innerText = aktif;
      document.getElementById("statNonAktif").innerText = non;
      document.getElementById("statTeregistrasi").innerText = tereg;
    }

    function renderCharts(rows) {
      const byKategori = {}, byDaftar = {};
      rows.forEach(r => {
        const k = r["Jenis/Fokus"] || "Tidak diketahui";
        byKategori[k] = (byKategori[k] || 0) + 1;
        const s = r["Status Pendaftaran"] || "Tidak diketahui";
        byDaftar[s] = (byDaftar[s] || 0) + 1;
      });
      new Chart(document.getElementById("chartKategori"), {type:"doughnut",data:{labels:Object.keys(byKategori),datasets:[{data:Object.values(byKategori)}]},options:{plugins:{legend:{position:"bottom"}}}});
      new Chart(document.getElementById("chartPendaftaran"), {type:"bar",data:{labels:Object.keys(byDaftar),datasets:[{data:Object.values(byDaftar)}]},options:{plugins:{legend:{display:false}},scales:{y:{beginAtZero:true}}}});
    }

    function groupByLocation(rows) {
      const groups = {};
      rows.forEach(r => {
        const lat = parseFloat(r["Latitude"]);
        const lng = parseFloat(r["Longitude"]);
        if (isNaN(lat) || isNaN(lng)) return;
        const key = lat.toFixed(5)+","+lng.toFixed(5);
        if (!groups[key]) groups[key] = {lat,lng,desa:r["Kantor/Sekretariat"]||"-",items:[]};
        groups[key].items.push(r);
      });
      return groups;
    }

    function badgeClass(g) {
      const total=g.items.length||1;
      const aktif=g.items.filter(i=>isActive(i["Status"])).length;
      const ratio=aktif/total;
      if(ratio>=0.6) return "badge-pin green";
      if(ratio>0 && ratio<0.6) return "badge-pin red";
      return "badge-pin";
    }

    function buildPopup(g) {
      return "<div><strong>"+(g.desa||"Lokasi")+"</strong><br/>Total: "+g.items.length+"</div>";
    }

    function initMap(rows) {
      const groups=groupByLocation(rows);
      const map=L.map('map').setView([-0.3245,127.8880],10);
      L.tileLayer('https://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png',{maxZoom:18,attribution:'&copy; OpenStreetMap'}).addTo(map);
      const bounds=[];
      Object.values(groups).forEach(g=>{
        const icon=L.divIcon({html:"<div class='"+badgeClass(g)+"'>"+g.items.length+"</div>",className:"",iconSize:[36,36],iconAnchor:[18,18]});
        L.marker([g.lat,g.lng],{icon}).addTo(map).bindPopup(buildPopup(g));
        bounds.push([g.lat,g.lng]);
      });
      if(bounds.length) map.fitBounds(bounds);
    }

    function renderTable(rows) {
      const data = rows.map(r => [r['No']||'',r['Nama Ormas/Lembaga']||'',r['Ketua/PJ']||'',r['Kontak']||'',r['Status']||'',r['Kantor/Sekretariat']||'',r['Jenis/Fokus']||'',r['Legalitas']||'',r['Status Pendaftaran']||'',r['Lingkup']||'',r['Metode Verifikasi']||'',r['Kegiatan']||'',r['Latitude']||'',r['Longitude']||'']);
      $('#ormasTable').DataTable({data, pageLength:25});
    }

    const rows=ROWS;
    summarize(rows);
    renderCharts(rows);
    initMap(rows);
    renderTable(rows);
  </script>
</body>
</html>
